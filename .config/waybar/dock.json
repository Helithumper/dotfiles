  "custom/ff": {
    "format": "",
    "tooltip": false,
    "on-click": "/home/peyton/.config/waybar/focus_app firefox '.window_properties.class == \"firefox\"'",
    "on-click-middle": "firefox",
    "on-click-right": "~/lib/cfg/waybar/focus_app -l '.window_properties.class == \"firefox\"'"
  },
  "custom/term": {
    "format": "",
    "tooltip": false,
    "on-click": "kitty",
    "on-click-middle": "kitty",
    "on-click-right": "~/lib/cfg/waybar/focus_app -l '.app_id == \"kitty\"'"
  },
  "custom/fm": {
    "format": "",
    "tooltip": false,
    "on-click": "~/lib/cfg/waybar/focus_app pcmanfm '.window_properties.class == \"Pcmanfm\"'",
    "on-click-middle": "pcmanfm",
    "on-click-right": "~/lib/cfg/waybar/focus_app -l '.window_properties.class == \"Pcmanfm\"'"
  },
  "custom/ed": {
    "format": "",
    "tooltip": false,
    "on-click": "~/lib/cfg/waybar/focus_app howl '.app_id == \"howl\"'",
    "on-click-middle": "howl",
    "on-click-right": "~/lib/cfg/waybar/focus_app -l '.app_id == \"howl\"'"
  },
  "custom/gimp": {
    "format": "",
    "tooltip": false,
    "on-click": "gimp",
    "on-click-middle": "gimp",
    "on-click-right": "gimp"
  },
  "custom/tg": {
    "format": "",
    "tooltip": false,
    "on-click": "telegram-desktop",
    "on-click-middle": "telegram-desktop",
    "on-click-right": "telegram-desktop"
  }
